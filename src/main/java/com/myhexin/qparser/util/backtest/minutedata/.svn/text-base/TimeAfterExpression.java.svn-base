package com.myhexin.qparser.util.backtest.minutedata;

/**
 * 用于保存 分时状态发生之后的 等待时间。 
 * 比如 macd金叉后 10分钟， 这里 的 TimeAfterExpression 就是10分钟。
 *
 */
public class TimeAfterExpression {
    
    private int time;

    public int getTime() {
        return time;
    }

    public void setTime(int time) {
        this.time = time;
    }

}
