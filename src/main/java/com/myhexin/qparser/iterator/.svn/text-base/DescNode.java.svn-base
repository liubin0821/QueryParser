package com.myhexin.qparser.iterator;

import com.myhexin.qparser.define.EnumDef.DescNodeType;

public class DescNode {
    private String text;		// 文本
	private DescNodeType type;	// 类型
	
    public DescNode(String text,DescNodeType type){
    	this.text = text;
    	this.type = type;
    }
    
    public String getText() {
		return text;
	}
    
	public void setText(String text) {
		this.text = text;	
	}
	
	public DescNodeType getType() {
		return type;
	}
	
	public void setType(DescNodeType type) {
		this.type = type;
	}
}
